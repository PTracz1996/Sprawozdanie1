\section{Formatowanie tekstu} 
\begin{itemize}
\huge \item  Czcionki \newline
\texttt{Typewriter} \\ \\ \\ \\ \\
\huge   Wielkosc czcionki \newline \\
\\
\normalsize Czcionka automatyczna \newline \\
\large  large  \newline \\
\small  small  \newline \\
\tiny Bardzo mala  \newline \\



\huge   Kolorowanie tekstu \newline \large
{\color{red}  czerwony} \\
{\color{green}  zielony}\\


\huge   Pogrubienie, kursywa, podkreslenia\newline \\ 
\large \textbf{Czcionka pogrubiona} \\
\textit{Czcionka pochylona}\\ 
\uline{pojedyncze podkreslenie}\\
\uuline{podw√≥jne podkreslenie}\\

 \huge Centrowanie tekstu\newline
\begin{flushleft}
\large Do lewej
\end{flushleft}
\begin{center}
\large Srodku
\end{center}
\begin{flushright}
\large DO prawej
\end{flushright}
\end{itemize}
\section{Formatowanie Tabel}
