\hline
7 & 8 & 9 \\
\hline 
ala&ma&kota\\
\hline 
\end{tabular}
\end{flushleft}


\section{Wzory}

$$
\lim_{n \to \infty}
\sum_{k=1}^n \frac{1}{k^2}
= \frac{\pi^3}{10}
$$
